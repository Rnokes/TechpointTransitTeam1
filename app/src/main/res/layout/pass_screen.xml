<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#47978B">

    <View
        android:id="@+id/top_back4"
        android:layout_width="417dp"
        android:layout_height="153dp"
        android:layout_marginBottom="578dp"
        android:background="@drawable/grey_rectangle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/indyGoDisplay"
        android:layout_width="263dp"
        android:layout_height="68dp"
        android:layout_marginBottom="469dp"
        android:layout_marginTop="194dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:importantForAutofill="no"
        android:text="@string/indygo"
        android:textAlignment="center"
        android:textSize="40sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="LabelFor" />

    <ImageButton
        android:id="@+id/menubutton3"
        android:layout_width="87dp"
        android:layout_height="62dp"
        android:layout_marginBottom="613dp"
        android:layout_marginEnd="293dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="56dp"
        android:contentDescription="@string/main_menu_desc"
        android:src="@drawable/menu_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/newbus4"
        android:layout_width="68dp"
        android:layout_height="96dp"
        android:layout_marginBottom="550dp"
        android:layout_marginEnd="172dp"
        android:layout_marginStart="171dp"
        android:layout_marginTop="50dp"
        android:contentDescription="@string/bus_icon_desc"
        android:src="@drawable/newbus"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="299dp"
        android:layout_height="285dp"
        android:layout_marginBottom="57dp"
        android:layout_marginTop="389dp"
        android:contentDescription="@string/qrholder"
        android:src="@drawable/white_rectangle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="298dp"
        android:layout_height="59dp"
        android:layout_marginBottom="371dp"
        android:layout_marginTop="301dp"
        android:contentDescription="@string/spinner_holder"
        android:src="@drawable/white_rectangle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="63dp"
        android:layout_height="59dp"
        android:layout_marginBottom="371dp"
        android:layout_marginEnd="292dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="301dp"
        android:contentDescription="@string/countholder"
        android:src="@drawable/white_rectangle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/qrCode"
        android:layout_width="288dp"
        android:layout_height="273dp"
        android:layout_marginBottom="63dp"
        android:layout_marginTop="395dp"
        android:contentDescription="@string/qrcode"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView11"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/passSpinner"
        android:layout_width="232dp"
        android:layout_height="50dp"
        android:layout_marginBottom="376dp"
        android:layout_marginEnd="61dp"
        android:layout_marginStart="118dp"
        android:layout_marginTop="305dp"
        android:background="@color/colorGreen"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/timeLeftCounter"
        android:layout_width="43dp"
        android:layout_height="31dp"
        android:layout_marginBottom="395dp"
        android:layout_marginStart="66dp"
        android:layout_marginTop="305dp"
        android:ems="10"
        android:importantForAutofill="no"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="LabelFor" />

    <TextView
        android:id="@+id/daysLeftCounter"
        android:layout_width="49dp"
        android:layout_height="16dp"
        android:layout_marginBottom="379dp"
        android:layout_marginStart="63dp"
        android:layout_marginTop="336dp"
        android:ems="10"
        android:importantForAutofill="no"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="LabelFor" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="109dp"
        android:layout_height="16dp"
        android:layout_marginBottom="597dp"
        android:layout_marginEnd="277dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="118dp"
        android:text="@string/main_menu_desc"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="10sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SmallSp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="144dp"
        android:layout_height="19dp"
        android:layout_marginBottom="430dp"
        android:layout_marginTop="282dp"
        android:text="@string/select_an_active_pass"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>